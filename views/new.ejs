<%- include("partials/header") %> 

<h1>Create New Capground</h1>


<div class="container">
    <form action="/campgrounds" method="POST">
        <div class="form-group">
          <label>Title</label>
          <input type="text" class="form-control" name="title" placeholder="Campground Name">
        </div>
        <div class="form-group">
            <label>Image</label>
            <input type="text" class="form-control" name="image" placeholder="Image url">
          </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>

<a href="/campgrounds">back to campground</a>




